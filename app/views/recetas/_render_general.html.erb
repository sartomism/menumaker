
<table>
	<tr>
		<td>
			<fieldset>
				<legend>Receta</legend>
				<% form_for(receta) do |f| %>
				  <%= f.error_messages %>
				
				  <p>
				    <%= f.label :nombre %><br />
				    <%= f.text_field :nombre %>
				  </p>
				  <p>
				    <%= f.submit "Guardar Cambios" %>
				  </p>
				  
				<% end %>
			</fieldset>
		</td>
	</tr>
</table>

